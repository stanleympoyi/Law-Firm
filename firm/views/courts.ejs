    <div class="container">
        <div class="row">
            <div class="col-md-4 off-md-3">
                <div class="signup-form">
                    <form action="" class="mt-5 border p-4 bg-light shadow mb-3">
                        <h3 class="mb-4 text-secondary">Add Court</h3>
                        <div class="row">
                            <div class="mb-3 col-md-6">
                                <label for="CourtID">Court ID</label>
                                <input type="text" name="CourtID" class="form-control" placeholder="CourtID" id="CourtID">
                            </div>
                            <div class="mb-3 col-md-6">
                                <label for="Courtname">Court Name</label>
                                <input type="text" name="Courtname" class="form-control" placeholder="Court Name" id="CourtID">
                            </div>
                            
                        </div>
                        <div class="mb-3 col-md-9">
                            <label for="address">Address</label>
                            <input type="text" name="address" class="form-control" placeholder="address" id="address">
                        </div>
                        <div class="row">
                            <div class="mb-3 col-md-6">
                                <label for="city">City</label>
                                <input type="text" name="city" class="form-control" placeholder="city" id="city">
                            </div>
                            <div class="mb-3 col-md-6">
                                <label for="ST">State</label>
                                <input type="text" name="st" class="form-control" placeholder="st" id="st">
                            </div>
                            <div class="mb-3 col-md-6">
                                <label for="Zipcode">Zip Code</label>
                                <input type="number" name="Zipcode" class="form-control" placeholder="Zipcode" id="Zipcode">
                            </div>
                            <div class="col-md-12">
                                <button class="btn btn-primary float-end">Add Court</button>
                            </div>
                        </div>
